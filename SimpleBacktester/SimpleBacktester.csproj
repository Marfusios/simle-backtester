  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SimpleBacktester.Visualization\SimpleBacktester.Visualization.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
